---
const {img1, img2, img3, img4, img5, img6, img7, img8} = Astro.props;
---

<div class="justify-center flex-initial lg:w-80">
    <div class="p-4 hover:scale-110  transition duration-700">
        <img src={img1}>
    </div>

    <div class="p-4 hover:scale-110  transition duration-700">
        <img src={img2}>
    </div>

    <div class="p-4 hover:scale-110  transition duration-700">
        <img src={img3}>
    </div>

    <div class="p-4 hover:scale-110  transition duration-700">
        <img src={img4}>
    </div>

    <div class="p-4 hover:scale-110  transition duration-700">
        <img src={img5}>
    </div>

    <div class="p-4 hover:scale-110  transition duration-700">
        <img src={img6}>
    </div>

    <div class="p-4 hover:scale-110  transition duration-700">
        <img src={img7}>
    </div>

    <div class="p-4 hover:scale-110  transition duration-700">
        <img src={img8}>
    </div>
</div>